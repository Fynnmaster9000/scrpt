-- Scripts:

local function OEPZR_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	-- LocalScript inside the TextButton
	local button = script.Parent -- The button itself
	local player = game.Players.LocalPlayer
	
	button.MouseButton1Click:Connect(function()
		-- Kicks the player without any message
		player:Kick()
	end)
	
end
coroutine.wrap(OEPZR_fake_script)()
local function LECSGAV_fake_script() -- sigmavirus.LocalScript 
	local script = Instance.new('LocalScript', sigmavirus)

	-- LocalScript inside the GUI or wherever you want to trigger the blur effect
	
	local player = game.Players.LocalPlayer
	local camera = game.Workspace.CurrentCamera
	local lighting = game:GetService("Lighting")
	local blur = Instance.new("BlurEffect")  -- Create a new BlurEffect instance
	
	-- Set the blur effect properties
	blur.Size = 22  -- Adjust the blur strength (0 to 25 range)
	blur.Parent = lighting  -- Add the blur effect to Lighting
	
	-- Create a ScreenGui and a TextButton for testing the GUI
	
end
coroutine.wrap(LECSGAV_fake_script)()
local function LRLW_fake_script() -- sigmavirus.LocalScript 
	local script = Instance.new('LocalScript', sigmavirus)

	-- LocalScript (to be placed in StarterGui)
	
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	
	-- Wait for the humanoid to load in the character
	local humanoid = character:WaitForChild("Humanoid")
	
	-- Disable movement
	humanoid.WalkSpeed = 0  -- Set WalkSpeed to 0 to stop movement
	humanoid.PlatformStand = true  -- Disable platform movement (prevents moving)
	
	-- Optionally, you can re-enable movement after a certain time or condition:
	-- wait(5)  -- Wait for 5 seconds
	-- humanoid.WalkSpeed = 16  -- Reset WalkSpeed to default
	-- humanoid.PlatformStand = false  -- Re-enable movement
	
end
coroutine.wrap(LRLW_fake_script)()
